 */


#include "PWM_CONTROLLER.h"

int main(void)
{
	while(1)
	{
		enablePWM(0xF);
		setPWMPulseWindow(1,100000);
		setPWMPulseWindow(2,100000);
		setPWMPulseWindow(3,100000);
		setPWMPulseWindow(4,100000);
		*(PWM_ADDR + 5) = 100000;
		*(PWM_ADDR + 6) = 7;
		*(PWM_ADDR + 7) = 4;
		*(PWM_ADDR + 8) = 2;
	}
}
