#include "PWM_CONTROLLER.h"

int main(void)
{
	while(1)
	{
		enablePWM(0xF);
		setPWMPulseWindow(1,10);
		setPWMPulseWindow(2,10);
		setPWMPulseWindow(3,10);
		setPWMPulseWindow(4,10);
		*(PWM_ADDR + 5) = 10;
		*(PWM_ADDR + 6) = 7;
		*(PWM_ADDR + 7) = 4;
		*(PWM_ADDR + 8) = 1;
	}
}
