#include "PWM_CONTROLLER.h"

int main(void)
{
	while(1)
	{
		enablePWM(0xF);
		setPWMPulseWindow(1,10);
		setPWMPulseWindow(2,10);
		setPWMPulseWindow(3,10);
		ssetPWMPulseWindow(4,10);
		setDC(1,100);
		setDC(2,75);
		setDC(3,50);
		setDC(4,25);
	}
}
