#include "PWM_CONTROLLER.h"

int main(void)
{
	while(1)
	{
		enablePWM(0xF);
		*(PWM_ADDR + 1) = 100;
		*(PWM_ADDR + 2) = 100;
		*(PWM_ADDR + 3) = 100;
		*(PWM_ADDR + 4) = 100;

		*(PWM_ADDR + 5) = 15;
		*(PWM_ADDR + 6) = 15;
		*(PWM_ADDR + 7) = 15;
		*(PWM_ADDR + 8) = 15;
	}
}
